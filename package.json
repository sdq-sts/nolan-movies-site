{
  "name": "nolan-movies",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Sadraque <sdq.sts@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env HOST=0.0.0.0 PORT=3333 nuxt",
    "test": "npm run test:unit -- npm run test:e2e",
    "test:unit": "TEST=unit ava ./test/specs/*.spec.js --verbose --serial",
    "test:e2e": "TEST=e2e ava ./test/e2e/*.spec.js --verbose",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "nuxt": "^1.4.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.19",
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.1",
    "browser-env": "^3.2.5",
    "cross-env": "^5.2.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jsdom": "^11.11.0",
    "node-sass": "^4.9.2",
    "nuxt-sass-resources-loader": "^2.0.2",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.0.1",
    "sass-loader": "^7.0.3"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/setup.js"
    ],
    "files": [
      "./test/**/*.spec.js"
    ]
  }
}
